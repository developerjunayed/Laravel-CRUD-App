{
    "version": 2,
    "framework": "null",
    "functions":{
        "api/index.php": { "runtime": "vercel-php@0.6.0"}
    },
    "routes": [
        
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],

    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "https://devjootest.vercel.app",
        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie"
    }
}